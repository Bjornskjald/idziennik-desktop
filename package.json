{
  "name": "idziennik-electron",
  "productName": "iDziennik",
  "version": "1.5.1",
  "description": "Wieloplatformowa aplikacja desktopowa dla modu≈Çu Node.js iDziennik",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bjornskjald/idziennik-electron.git"
  },
  "author": {
    "name": "Bjornskjald",
    "email": "github@bjorn.ml"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bjornskjald/idziennik-electron/issues"
  },
  "build": {
    "appId": "ml.bjorn.idziennik.electron",
    "files": [
      "css/",
      "js/",
      "html/",
      "fonts/",
      "icons/",
      "node_modules/",
      "main.html",
      "package.json",
      "index.js"
    ],
    "linux": {
      "category": "Utility;Network",
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icons/icon.ico"
    }
  },
  "homepage": "https://github.com/Bjornskjald/idziennik-electron#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "jquery": "^3.2.1",
    "materialize-css": "^0.98.2"
  },
  "devDependencies": {
    "electron": "^1.6.10",
    "electron-builder": "^18.5.1"
  }
}
